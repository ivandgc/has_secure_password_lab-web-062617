<h1>Create User</h1>
<p>send post request to users#create</p>
<%= form_for @user do |f| %>
  <%= f.label "Name" %>
  <%= f.text_field :name %>
  <%= f.label "Password" %>
  <%= f.password_field :password %>
  <%= f.label "Confirm Password" %>
  <%= f.password_field :password_confirmation %>
  <%= f.submit %>
<% end %>
